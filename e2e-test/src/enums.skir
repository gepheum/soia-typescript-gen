/// A day of the week.
enum Weekday {
  MONDAY;
  TUESDAY;
  WEDNESDAY;
  THURSDAY;
  FRIDAY;
  SATURDAY;
  /// Fun day
  SUNDAY;
}

enum JsonValue {
  NULL = 1;
  /// true or
  /// false
  boolean: bool = 100;
  number: float64 = 6;
  string: string = 3;
  array: [JsonValue] = 4;
  struct Pair {
    name: string = 0;
    value: JsonValue = 1;
  }
  object: [Pair|name] = 5;
}

enum EmptyEnum {}

enum EnumWithNameConflict {
  SERIALIZER;
}
