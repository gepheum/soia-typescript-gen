  "author": "Tyler Fibonacci <gepheum@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "build:e2e-test": "tsc -b e2e-test",
    "format-soia": "cd e2e-test && soiac --fmt fix && cd ..",
    "format-soia:check": "cd e2e-test && soiac --fmt check && cd ..",
    "format": "prettier --write \"**/*.ts\" && npm run format-soia",
    "format:check": "prettier --check \"**/*.ts\" && npm run format-soia:check",
    "test": "npm run build && cd e2e-test && soiac -r . && npm run build:e2e-test && cd .. && mocha",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "buckwheat": "^1.1.2",
    "eslint": "^8.54.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^4.2.0",
    "soia": "^1.2.0",
    "soiac": "^1.0.51",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
