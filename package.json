    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "buckwheat": "^1.1.2",
    "mocha": "^10.2.0",
    "prettier": "^3.2.4",
    "soia": "^1.0.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
