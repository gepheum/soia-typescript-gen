{
  "compilerOptions": {
    "lib": ["ES2020"],
    "target": "ES2020",
    "sourceMap": true,
    "composite": true,
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowUnreachableCode": true,
    "skipLibCheck": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
  },
}
